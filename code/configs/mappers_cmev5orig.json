{
  "preprocess": [
      { "type": "drop-nan" },
      { "type": "drop-lowvar", "var": 1e-2 },
      { "type": "zscore" }
  ],
  "mappers": [{
    "type": "CustomMapper",
    "name": "BDLMapperOrig",
    "preprocess_type": "none",
    "dist_type": ["cityblock", "euclidean"],
    "embed_type": "CMDS",
    "embed_dim": 2,
    "rknn_type": "original",
    "binning_type": "Nd",
    "binning_resolution": [10, 20, 30],
    "binning_gain": [30, 40, 50, 70, 80],
    "binning_nsides": [3, 4],
    "clustering_type": "linkage_histo",
    "clustering_histo_bins": 10,
    "finalgraph_type": "full"
  }],
  "analyses": [
    { "type": "plot_graph" },
    { "type": "compute_stats", "args": { "HRF_threshold": 11 } },
    { "type": "compute_temp", "args": { "skip_mat": true } },
    { "type": "plot_task", "args": { "name": "CME", "path": "/oak/stanford/groups/saggar/data-cme-shine375/timing.csv" } }
  ]
