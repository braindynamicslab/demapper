{
  "preprocess": [
      { "type": "drop-nan" },
      { "type": "drop-lowvar", "var": 1e-2 },
      { "type": "zscore" }
  ],
  "mappers": [{
    "type": "CustomMapper",
    "name": "umapBDLMapperPrep",
    "preprocess_type": "none",
    "dist_type": "euclidean",
    "prelens_type": "preprocessed",
    "embed_type": "UMAP",
    "embed_kparam": [6, 12, 32],
    "embed_dim": [2, 2],
    "binning_type": "Nd",
    "binning_resolution": [10, 15, 20, 25, 30],
    "binning_gain": [30, 40, 50, 60, 70],
    "binning_nsides": 4,
    "clustering_type": "linkage_histo",
    "clustering_histo_bins": 10,
    "finalgraph_type": "full"
  }],
  "analyses": [
    { "type": "compute_stats", "args": { "HRF_threshold": 11 } }
  ]
}
