{
  "preprocess": [
      { "type": "drop-nan" },
      { "type": "drop-lowvar", "var": 1e-2 },
      { "type": "zscore" }
  ],
  "mappers": [{
    "type": "NeuMapper",
    "k": [8, 11, 16, 24, 32],
    "resolution": [150, 200, 250, 300, 350],
    "gain": [26, 30, 34, 38, 42, 46, 50]
  }],
  "analyses": [
    { "type": "plot_graph" },
    { "type": "compute_stats", "args": { "HRF_threshold": 11 } },
    { "type": "compute_temp", "args": { "skip_mat": true } }
  ]
}
