{
  "preprocess": [
      { "type": "drop-nan" },
      { "type": "drop-lowvar", "var": 1e-2 },
      { "type": "zscore" }
  ],
  "mappers": [{
    "type": "BDLMapper",
    "k": [8, 12, 16, 24, 32, 48],
    "resolution": [10, 15, 20, 25, 30, 35, 40],
    "gain": [50, 60, 70, 80, 90]
  }],
  "analyses": [
    { "type": "plot_graph" },
    { "type": "compute_stats", "args": { "HRF_threshold": 11 } },
    { "type": "compute_temp" }
  ]
}
